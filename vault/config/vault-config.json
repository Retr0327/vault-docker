{
    "storage": {
        "mysql": {
            "username": "<mysql-user-name>",
            "password": "<mysql-password>",
            "database": "<mysql-database>",
            "address": "mysql:3306",
            "table": "<table-for-vault>",
            "ha_enabled": "true",
            "lock_table": "<table-for-ha-info>"
